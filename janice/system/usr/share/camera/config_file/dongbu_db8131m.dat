//=========================================================
// Macro definition accessing I2C MIPI interface (0x78)
// => No inversion while reading and writing
//=========================================================
GET_REG=

  $"[DB8131M] >> GET_REG"

  $f FF !0 $f // Set Page Mode
  $f !1 $r2 0 1 $f // Read Address
  $"GET_REG byte byte 0 %0 1 %1"
  
  $"[DB8131M] << GET_REG"
;

//=========================================================
// Check that the device is alive by reading the device id
