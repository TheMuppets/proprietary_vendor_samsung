$"[DB8131M] >> REG_FLICKER_50HZ"
FF 82 $f //Page mode
92 59 $f //AeCtrl
;

REG_FLICKER_60HZ =
$"[DB8131M] >> REG_FLICKER_60HZ"
FF 82 $f //Page mode
92 5D $f //AeCtrl
;



