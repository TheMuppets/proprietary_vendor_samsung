seq_ss_start =
$"[ISX012] >> seq_ss_start"
$d000a
#WAIT_AF_END
$ce.00 0  //flash off cmd
 $"[ISX012] >> seq_ss_start flash off"
 #GET_REG_8(01A5)	
 $cg0 2B  // low lux
  $cf 03  //night mode
   #ISX012_LOWLUX_NIGHT_CAPTURE_MODE
  $cl //!nigh mode
   #REG_CAPTURE_MODE
  $cx  //~nigh mode
 $cl // !low lux
  #REG_CAPTURE_MODE
 $cx  // ~low lux
 #WAIT_FOR_CAMERA_MODE_TRANSITION
 //#WAIT_FOR_VINT
$cx  //flash off cmd
//////////////////////////////////////
$ce.00 1 //flash on cmd
 $"[ISX012] >> seq_ss_start flash on"
 $ce.05 1  //touch AF
  0294 02 $f        
  0297 02 $f     
  029A 02 $f        
  029E 02 $f   
  $d0042  //66ms                  
 $cx  //touch af
 $gf 01   //max flash.
 #REG_CAPTURE_MODE
 #WAIT_FOR_CAMERA_MODE_TRANSITION
 #WAIT_FOR_VINT
 $d00d2 // Wait 1V time(210ms)
 $s5 01
 [5 50 #READ_AWBSTS]
$cx  //flash on cmd

$"[ISX012] << seq_ss_start"
;	 
       
seq_ss_stop =
$"[ISX012] >> seq_ss_stop"
$ce.00 1 //flash on status
 $gf 00  //flash off
 #ISX012_FLASH_OFF_RESET
 $ce.01 0  //if !mwb status
  0282 20 $f
 $cx //if !mwb status end
 8800 01  $f
$cx //flash on status end

$ce.02 1  //range auto 
 #ISX012_AF_MACRO_OFF
$cl  //range auto : macro
 #ISX012_AF_MACRO_ON 
$cx  //range auto end
 
$"[ISX012] << seq_ss_stop"
;

