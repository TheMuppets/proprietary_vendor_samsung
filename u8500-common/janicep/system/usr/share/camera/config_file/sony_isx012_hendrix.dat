//==================================================//
// Macro definition accessing I2C MIPI interface	//
// => No inversion while reading and writing		//
//==================================================//

//--------------//
//	GET_REG		//
//--------------//
GET_REG_8=
$f !0 $r1 0	// read 2 byte from cam reg !0 
//$"GET_REG_8  byte 0 %0"
;

GET_REG_16=
$f !0 $r2 0	1// read 2 byte from cam reg !0 
$<1 8
$+0 r1
//$"GET_REG_16  byte 0 %0"
;

//--------------------------//
//	CHECK_OM_CHANGED_BIT	//
//--------------------------//
CHECK_OM_CHANGED_BIT =
//$"[ISX012] >> CHECK_OM_CHANGED_BIT"
$d0005	// Delay 5ms
000E $r1 6 $f
$c6.00 1
	//$"	[ISX012] INTSTS0 status after transition: %6"
	$s5 00
$cl
	//$"	[ISX012] INTSTS0 status on transition: %6"
	$s5 01
	$d001E	// Delay 30ms
$cx
//$"[ISX012] << CHECK_OM_CHANGED_BIT"
;

//--------------//
//	OM_CHANGE	//
//--------------//
OM_CHANGE =
//$"[ISX012] >> OM_CHANGE"
0012 01 $f  //clear interrupt status
$d0005	// Delay 5ms
000E $r1 6 $f
$c6.00 0
	//$"	[ISX012] OM_CHANGE status after transition: %6"
	$s5 00
$cl
	//$"[ISX012] OM_CHANGE status on transition: %6"
	$s5 01
	$d001E	// Delay 30ms
$cx
//$"[ISX012] << OM_CHANGE"
;

//--------------------------------------//
//	WAIT_FOR_OPERATION_MODE_TRANSITION	//
//--------------------------------------//
WAIT_FOR_OPERATION_MODE_TRANSITION = 
$"[ISX012] >> WAIT_OM"
$s5 01
[5 FF #CHECK_OM_CHANGED_BIT]
$s5 01
[5 FF #OM_CHANGE]
//$"[ISX012] << WAIT_OM"
; 

