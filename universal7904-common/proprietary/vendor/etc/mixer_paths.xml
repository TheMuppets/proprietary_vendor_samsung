	<path name="incall-rec-uplink-and-downlink-bt-sco">
	</path>

	<!-- output device -->
	<path name="dev-handset">
		<ctl name="EP EP On" value="1" />
	</path>

	<path name="dev-speaker">
	</path>

	<path name="dev-headset">
