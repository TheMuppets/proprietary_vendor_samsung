# Create the directories used by DPM subsystem

#start dataadpl service
service vendor.dataadpl /system/vendor/bin/adpl
    class main
    user radio
    socket adpl_cmd_uds_file dgram 660 radio radio
    group radio oem_2901 diag usb inet
    disabled